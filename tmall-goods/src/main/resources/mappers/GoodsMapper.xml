<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tmall.goods.mapper.GoodsMapper">


    <insert id="saveGoodsDto" useGeneratedKeys="true" keyProperty="id" parameterType="com.tmall.goods.mapper.dto.GoodsDto">
        INSERT INTO `goods_details` (`goods_name`,`goods_description`,`goods_image_urls`,`goods_price`,`store_id`)
        VALUES (#{goodsName},#{goodsDiscription},#{goodsPrice},#{goodsImageUrls},#{storeId})
    </insert>


</mapper>